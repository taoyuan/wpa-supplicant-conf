{
  "name": "wpa-supplicant-conf",
  "version": "0.1.0",
  "description": "wpa_supplicant config management",
  "repository": "taoyuan/wpa-supplicant-conf",
  "author": "Yuan Tao",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "jsnext:main": "index.es6.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "bluebird": "^3.4.1",
    "child-process-promise": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.11",
    "del": "^2.2.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "nock-exec": "aledlewis/nock-exec",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.6.1",
    "sinon": "^2.0.0-pre"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepublish": "npm run build",
    "publish:docs": "easystatic deploy docs --repo taoyuan/wpa-supplicant-conf",
    "start": "easystatic start docs"
  }
}
